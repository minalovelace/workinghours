<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project workinghours">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="${dir.buildfile}/../.."/>
    <property name="dir.jarfile" value="/Users/Nina/Desktop"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/Workinghours.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="workinghours.Main"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.workspace}/workinghours/bin"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.workspace}/GSON_external_src/gson-2.6.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.workspace}/GSON_external_src/gson-2.6.1-sources.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.workspace}/GSON_external_src/gson-2.6.1-javadoc.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.workspace}/org.eclipse.swt/swt.jar"/>
        </jar>
    </target>
</project>
